from django.db import models

class homepage(models.Model):
    carouselimagename1 = models.CharField(max_length=80)
    carouselimagename2 = models.CharField(max_length=80)
    carouselimagename3 = models.CharField(max_length=80)
    carouselcaption1 = models.CharField(max_length=50)
    carouselcaption2 = models.CharField(max_length=50)
    carouselcaption3 = models.CharField(max_length=50)
    
    website = models.URLField()

    def __unicode__(self):
        return self.name
	class meta:
		ordering=['name']
